{
  "name": "charlin-react-native-interview",
  "description": "React Native Boilerplate, TypeScript with React Native Navigation",
  "keywords": [
    "react native",
    "typescript",
    "boilerplate",
    "react native navigation"
  ],
  "version": "1.0.0-development",
  "private": true,
  "scripts": {
    "start": "react-native start",
    "start-no-cache": "react-native start \"--reset-cache\"",
    "ios": "react-native run-ios",
    "android": "react-native run-android",
    "android-shake": "adb shell input keyevent 82",
    "reset": "react-native start --reset-cache",
    "test": "jest",
    "lint": "eslint . --ext ts --ext tsx --ext js --ext jsx",
    "lint:fix": "yarn lint --fix",
    "generate": "graphql-codegen --config codegen.js",
    "restart": "watchman watch-del-all && rm -rf $TMPDIR/react-* && rm -rf node_modules/ && yarn cache clean && yarn install && yarn run android"
  },
  "dependencies": {
    "@apollo/react-hooks": "^3.1.3",
    "@graphql-codegen/cli": "^1.12.2",
    "@graphql-codegen/introspection": "^1.12.2",
    "@graphql-codegen/typescript": "^1.12.2",
    "@graphql-codegen/typescript-operations": "^1.12.2",
    "@graphql-codegen/typescript-react-apollo": "^1.12.2",
    "@react-native-community/toolbar-android": "^0.1.0-rc.2",
    "apollo": "^2.22.0",
    "apollo-cache-inmemory": "^1.6.3",
    "apollo-client": "^2.6.4",
    "apollo-link": "^1.2.12",
    "apollo-link-http": "^1.5.15",
    "apollo-link-http-common": "^0.2.15",
    "apollo-utilities": "^1.3.3",
    "graphql": "^14.4.2",
    "graphql-tag": "^2.10.1",
    "graphql-tools": "^4.0.7",
    "react": "16.9.0",
    "react-native": "0.61.5",
    "react-native-dotenv": "^0.2.0",
    "react-native-gesture-handler": "^1.7.0",
    "react-native-paper": "3.6.0",
    "react-native-vector-icons": "^6.6.0",
    "react-navigation": "^4.0.10"
  },
  "license": "MIT",
  "devDependencies": {
    "@apollo/client": "^3.1.3",
    "@testing-library/jest-native": "^3.3.0",
    "@testing-library/react-native": "^7.0.1",
    "@types/jest": "^23.3.13",
    "@types/react": "^16.7.21",
    "@types/react-native": "^0.57.32",
    "@types/react-native-dotenv": "^0.2.0",
    "babel-jest": "^26.3.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "eslint": "^6.6.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jest": "^22.1.2",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.12.2",
    "eslint-watch": "^4.0.2",
    "jest": "^26.4.0",
    "prettier": "^2.0.5",
    "react-addons-test-utils": "^15.6.2",
    "react-native-typescript-transformer": "^1.2.11",
    "react-test-renderer": "16.9.0",
    "regenerator-runtime": "^0.13.1",
    "ts-jest": "^26.2.0",
    "tslint": "^5.16.0",
    "tslint-config-airbnb": "^5.11.1",
    "typescript": "^3.4.5"
  },
  "engines": {
    "node": ">=12.0.0",
    "npm": ">=6.0.0",
    "yarn": ">=1.9.2"
  }
}
